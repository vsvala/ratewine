<table class="table table-hover">
  <thead>
  <tr>
    <th>Name</th>
    <th>Year</th>
  </tr>
  </thead>

  <tbody>
  <% wineyards.each do |wineyard| %>
      <tr>
        <td><%= link_to wineyard.name, wineyard %></td>
        <td><%= wineyard.year %></td>
        <td></td>
      </tr>
  <% end %>
  </tbody>
</table>
